{
  "name": "plaid-node-walkthrough",
  "version": "0.1.1",
  "description": "A sample app and accompanying walk-through for the Plaid API using typescript.",
  "main": "src/index",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc src/*.ts",
    "start": "ts-node --project tsconfig.json ./src/index.ts",
    "dev": "nodemon tsc --project tsconfig.json ./src/index.ts",
    "watch": "nodemon src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "elingerian",
  "repository": "https://github.com/elnigerian/plaid_quickstart_node.git",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.18.x",
    "dotenv": "^8.2.0",
    "ejs": "2.5.x",
    "express": "4.16.x",
    "moment": "2.22.x",
    "nodemon": "^2.0.5",
    "plaid": "6.x.x"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.16",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": "12.x"
  }
